        "1ca044e7-16a1-418d-b66f-05ae52069d07": {
          "type": "accordion",
          "settings": {
            "title": "Installation Instructions",
            "icon": "check_mark",
            "content": "<p>Each Wooden Pegboard comes with all the necessary hardware to get going, with handy installation instructions below.<\/p><p><a href=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0186\/5604\/files\/Wooden_Pegboard_Instructions_Web.pdf?v=1632261343\" target=\"_blank\">Download Instructions PDF<\/a><\/p>",
